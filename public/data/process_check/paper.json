{
  "name": "过程检测/纸张",
  "api": {
    "insert": {
      "url": "754/li4y6pztli",
      "param": ["rec_time", "uid"]
    },
    "delete": {
      "url": "755/li4y6pztli",
      "param": ["machine_name", "check_date", "reel_no", "prod_name"]
    },
    "update": {
      "url": "756/li4y6pztli",
      "param": ["machine_name", "check_date", "reel_no", "prod_name"]
    },
    "query": {
      "url": "757/li4y6pztli",
      "param": ["machine_name", "check_date", "reel_no", "prod_name"]
    },
    "table": {
      "url": "758/li4y6pztli"
    },
    "load": { "url": "812/4ihq3k92zrl.json", "param": ["_id"] }
  },
  "table": "process_check_paper",
  "detail": [
    {
      "title": "基础数据",
      "detail": [
        {
          "title": "检测日期",
          "type": "datepicker",
          "key": "check_date",
          "datetype": "YYYY-MM-DD",
          "rule": {
            "required": true
          },
          "unReset": true
        },
        {
          "title": "机台号",
          "type": "radio.button",
          "key": "machine_name",
          "rule": {
            "required": true
          },
          "defaultOption": [
            {
              "name": "新线-1号",
              "value": "新线-1号"
            },
            {
              "name": "新线-2号",
              "value": "新线-2号"
            },
            {
              "name": "老线-3号",

              "value": "老线-3号"
            }
          ]
        },
        {
          "title": "轴号",
          "type": "input",
          "key": "reel_no",
          "rule": {
            "type": "reel",
            "required": true
          }
        },
        {
          "title": "品种",
          "type": "select",
          "key": "prod_name",
          "rule": {
            "required": true
          },
          "defaultOption": [
            "103-G-2T",
            "103-G-2A",
            "103-G-3TZ",
            "103-G-4T",
            "103-G-6T",
            "103-G-7T",
            "试验品"
          ]
        }
      ]
    },
    {
      "title": "检测详情",
      "detail": [
        {
          "title": "上胶量",
          "type": "input",
          "key": "glue_num",
          "block": "g/m<sup>2</sup>",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "成纸pH值",
          "type": "input",
          "key": "ph",
          "rule": {
            "type": "float",
            "required": true
          },
          "scope": {
            "max": 8.5,
            "min": 6.5
          }
        },
        {
          "title": "成纸灰份",
          "type": "input",
          "key": "grey",
          "rule": {
            "type": "float",
            "required": true
          },
          "suffix": "%"
        },
        {
          "title": "检验员",
          "type": "select",
          "key": "operator",
          "span": 8,
          "rule": {
            "required": true
          },
          "unReset": true,
          "url": "./data/base/paper_material_check_user.json"
        },
        {
          "title": "检验结论",
          "type": "switch",
          "key": "result",
          "checkedChildren": "合格",
          "unCheckedChildren": "不合格",
          "placeholder": "请在此填入检验结论",
          "span": 8,
          "rule": {
            "required": true
          },
          "defaultValue": true,
          "unReset": true
        },
        {
          "title": "备注",
          "type": "input.textarea",
          "key": "remark",
          "placeholder": "请在此填入备注信息",
          "span": 8,
          "rule": {
            "required": false
          }
        }
      ]
    }
  ]
}
