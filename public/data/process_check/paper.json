{
  "name": "过程检测/纸张",
  "api": {
    "insert": {
      "url": "754/li4y6pztli",
      "param": ["rec_time", "uid"]
    },
    "delete": {
      "url": "755/li4y6pztli",
      "param": ["machine_name", "check_date", "reel_no", "prod_name"]
    },
    "update": {
      "url": "756/li4y6pztli",
      "param": ["machine_name", "check_date", "reel_no", "prod_name"]
    },
    "query": {
      "url": "757/li4y6pztli",
      "param": ["machine_name", "check_date", "reel_no", "prod_name"]
    },
    "table": {
      "url": "758/li4y6pztli"
    }
  },
  "table": "process_check_paper",
  "detail": [
    {
      "title": "基础数据",
      "detail": [
        {
          "title": "检测日期",
          "type": "datepicker",
          "key": "check_date",
          "datetype": "YYYY-MM-DD",
          "rule": {
            "required": true
          },
          "unReset": true
        },
        {
          "title": "机台号",
          "type": "input",
          "key": "machine_name",
          "rule": {
            "required": true
          }
        },
        {
          "title": "轴号",
          "type": "input",
          "key": "reel_no",
          "rule": {
            "type": "reel",
            "required": true
          }
        },
        {
          "title": "品种",
          "type": "input",
          "key": "prod_name",
          "rule": {
            "required": true
          },
          "defaultOption": [
            {
              "name": "103-G-2A",
              "value": "103-G-2A"
            },
            {
              "name": "103-G-2T",
              "value": "103-G-2T"
            },
            {
              "name": "103-G-3A",
              "value": "103-G-3A"
            },
            {
              "name": "103-G-3T",
              "value": "103-G-3T"
            },
            {
              "name": "103-G-4T",
              "value": "103-G-4T"
            },
            {
              "name": "103-G-6T",
              "value": "103-G-6T"
            },
            {
              "name": "103-G-7T",
              "value": "103-G-7T"
            }
          ]
        }
      ]
    },
    {
      "title": "检测详情",
      "detail": [
        {
          "title": "上胶量",
          "type": "input",
          "key": "glue_num",
          "block": "g/m<sup>2</sup>",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "成纸pH值",
          "type": "input",
          "key": "ph",
          "rule": {
            "type": "float",
            "required": true
          },
          "scope": {
            "max": 8.5,
            "min": 6.5
          }
        },
        {
          "title": "成纸灰份",
          "type": "input",
          "key": "grey",
          "rule": {
            "type": "float",
            "required": true
          },
          "suffix": "%"
        },
        {
          "title": "检验员",
          "type": "select",
          "key": "operator",
          "span": 8,
          "rule": {
            "required": true
          },
          "unReset": true,
          "defaultOption": [
            { "name": "李虹斌", "value": "李虹斌" },
            { "name": "杨阳", "value": "杨阳" },
            { "name": "何馥利", "value": "何馥利" },
            { "name": "吴艳", "value": "吴艳" },
            { "name": "夏继红", "value": "夏继红" },
            { "name": "徐雯君", "value": "徐雯君" },
            { "name": "肖平", "value": "肖平" },
            { "name": "李京燕", "value": "李京燕" },
            { "name": "唐玲", "value": "唐玲" },
            { "name": "罗莉", "value": "罗莉" },
            { "name": "奚山鹰", "value": "奚山鹰" },
            { "name": "杨靖姝", "value": "杨靖姝" }
          ]
        },
        {
          "title": "检验结论",
          "type": "switch",
          "key": "result",
          "checkedChildren": "合格",
          "unCheckedChildren": "不合格",
          "placeholder": "请在此填入检验结论",
          "span": 8,
          "rule": {
            "required": true
          },
          "defaultValue": true,
          "unReset": true
        },
        {
          "title": "备注",
          "type": "input.textarea",
          "key": "remark",
          "placeholder": "请在此填入备注信息",
          "span": 8,
          "rule": {
            "required": true
          }
        }
      ]
    }
  ]
}
