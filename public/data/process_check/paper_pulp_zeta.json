{
  "name": "过程检测/过程浆料Zeta和PCD检测",
  "api": {
    "insert": {
      "url": "749/yjbm79kskh",
      "param": ["rec_time", "uid"]
    },
    "delete": {
      "url": "750/yjbm79kskh",
      "param": ["sample_date", "machine_name"]
    },
    "update": {
      "url": "751/yjbm79kskh",
      "param": ["sample_date", "machine_name"]
    },
    "query": {
      "url": "752/yjbm79kskh",
      "param": ["sample_date", "machine_name"]
    },
    "table": {
      "url": "753/yjbm79kskh"
    },
    "load": { "url": "811/cuvuni9uks.json", "param": ["_id"] }
  },
  "table": "process_check_paper_pulp_zeta",
  "detail": [
    {
      "title": "基础数据",
      "detail": [
        {
          "title": "取样时间",
          "type": "datepicker",
          "key": "sample_date",
          "datetype": "YYYY-MM-DD",
          "rule": {
            "required": true
          },
          "unReset": true
        },
        {
          "title": "机台号",
          "type": "select",
          "defaultOption": [
            { "name": "1#机", "value": "1#机" },
            { "name": "2#机", "value": "2#机" },
            { "name": "3#机", "value": "3#机" }
          ],
          "key": "machine_name",
          "rule": {
            "required": true
          }
        }
      ]
    },
    {
      "title": "检测详情",
      "detail": [
        {
          "title": "调和后浆PCD",
          "type": "input",
          "key": "pcd",
          "suffix": "μeq/L",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "调和后浆Zeta",
          "type": "input",
          "key": "zeta",
          "suffix": "mV",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "上网浆PCD",
          "type": "input",
          "key": "pcd_silk",
          "suffix": "μeq/L",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "上网浆Zeta",
          "type": "input",
          "key": "zeta_silk",
          "suffix": "mV",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "白水PCD",
          "type": "input",
          "key": "pcd_water",
          "suffix": "mV",
          "rule": {
            "type": "float",
            "required": true
          }
        },
        {
          "title": "检验员",
          "type": "select",
          "key": "operator",
          "span": 8,
          "rule": {
            "required": true
          },
          "unReset": true,
          "defaultOption": [
            { "name": "李虹斌", "value": "李虹斌" },
            { "name": "杨阳", "value": "杨阳" },
            { "name": "何馥利", "value": "何馥利" },
            { "name": "吴艳", "value": "吴艳" },
            { "name": "夏继红", "value": "夏继红" },
            { "name": "徐雯君", "value": "徐雯君" },
            { "name": "肖平", "value": "肖平" },
            { "name": "李京燕", "value": "李京燕" },
            { "name": "唐玲", "value": "唐玲" },
            { "name": "罗莉", "value": "罗莉" },
            { "name": "奚山鹰", "value": "奚山鹰" },
            { "name": "杨靖姝", "value": "杨靖姝" }
          ]
        },
        {
          "title": "检验结论",
          "type": "switch",
          "key": "result",
          "checkedChildren": "合格",
          "unCheckedChildren": "不合格",
          "placeholder": "请在此填入检验结论",
          "span": 8,
          "rule": {
            "required": true
          },
          "defaultValue": true,
          "unReset": true
        },
        {
          "title": "备注",
          "type": "input.textarea",
          "key": "remark",
          "placeholder": "请在此填入备注信息",
          "span": 8,
          "rule": {
            "required": true
          }
        }
      ]
    }
  ]
}
