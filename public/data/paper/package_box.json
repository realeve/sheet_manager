{
  "name": "钞纸成品装箱记录",
  "api": {
    "insert": {},
    "delete": {},
    "update": {},
    "query": {},
    "load": {}
  },
  "table": "package_box",
  "beforeInsert": "/data/paper/package_box.js",
  "detail": [
    {
      "title": "基础数据",
      "detail": [
        {
          "title": "产品名称",
          "type": "select",
          "unReset": true,
          "defaultOption": [
            {
              "name": "103-G-2T",
              "value": "103-G-2T"
            },
            {
              "name": "103-G-2A",
              "value": "103-G-2A"
            },
            {
              "name": "103-G-3T",
              "value": "103-G-3T"
            },
            {
              "name": "103-G-4T",
              "value": "103-G-4T"
            },
            {
              "name": "103-G-6T",
              "value": "103-G-6T"
            },
            {
              "name": "103-G-7T",
              "value": "103-G-7T"
            }
          ],
          "key": "prod_name",
          "rule": {
            "required": true
          },
          "span": 6
        },
        {
          "title": "码包时间",
          "type": "datepicker",
          "key": "package_date",
          "datetype": "YYYY-MM-DD",
          "rule": {
            "required": true
          },
          "unReset": true,
          "span": 6
        },
        {
          "title": "班次",
          "type": "input",
          "key": "class_type",
          "rule": {
            "required": true,
            "msg": "请输入班次"
          },
          "placeholder": "请输入班次",
          "unReset": true,
          "span": 6
        },
        {
          "title": "班别",
          "type": "input",
          "key": "class_name",
          "rule": {
            "required": true,
            "msg": "请输入班别"
          },
          "placeholder": "请输入班别",
          "unReset": true,
          "span": 6
        },
        {
          "title": "查纸1",
          "type": "input",
          "key": "user_check_paper1",
          "rule": {
            "required": true
          },
          "placeholder": "请输入查纸人员",
          "unReset": true,
          "span": 6
        },
        {
          "title": "查纸2",
          "type": "input",
          "key": "user_check_paper2",
          "rule": {
            "required": true
          },
          "placeholder": "请输入查纸人员",
          "unReset": true,
          "span": 6
        },
        {
          "title": "罩口袋",
          "type": "input",
          "key": "user_package",
          "rule": {
            "required": true
          },
          "placeholder": "请输入罩口袋人员",
          "unReset": true,
          "span": 6
        },
        {
          "title": "记轴号",
          "type": "input",
          "key": "user_record_reel",
          "rule": {
            "required": true
          },
          "placeholder": "请输入罩记轴号人员",
          "unReset": true,
          "span": 6
        },
        {
          "title": "记轴号2",
          "type": "input",
          "key": "user_record_reel2",
          "rule": {
            "required": true
          },
          "placeholder": "请输入罩记轴号人员",
          "unReset": true,
          "span": 6
        },
        {
          "title": "码垛工艺",
          "type": "radio.button",
          "key": "proc_type",
          "defaultOption": [
            {
              "value": "入立体库",
              "name": "入立体库",
              "scope": [
                {
                  "key": "ream_count",
                  "max": 10,
                  "min": 0
                }
              ],
              "hide": ["patch_no", "box_no", "pallet_no"]
            },
            {
              "value": "外调",
              "name": "外调",
              "scope": [
                {
                  "key": "ream_count",
                  "max": 36,
                  "min": 0
                }
              ],
              "hide": ["cart_num"]
            }
          ],
          "rule": {
            "required": true
          },
          "unReset": true,
          "span": 6
        },
        {
          "title": "批号",
          "type": "input",
          "key": "patch_no",
          "rule": {
            "type": "patch_no",
            "required": true
          },
          "span": 6,
          "unReset": true,
          "toupper": "true"
        },
        {
          "title": "箱号",
          "type": "input.number",
          "key": "box_no",
          "rule": {
            "type": "box_no",
            "required": true
          },
          "span": 6,
          "increase": "box_no"
        },
        {
          "title": "车号",
          "type": "input",
          "key": "cart_num",
          "rule": {
            "type": "reel_cart",
            "required": true
          },
          "span": 6,
          "increase": "reel_cart",
          "toupper": "true"
        },
        {
          "title": "拍号",
          "type": "input",
          "key": "pallet_no",
          "rule": {
            "type": "pallet_no",
            "required": true
          },
          "span": 6,
          "increase": "pallet",
          "toupper": "true"
        },
        {
          "title": "小计令数",
          "type": "input",
          "key": "ream_count",
          "block": "小计令数=令数1+令数2+令数3+令数4+令数5+令数6+令数7+令数8+令数9",
          "rule": {
            "type": "int",
            "required": true
          },
          "span": 6,
          "defaultValue": 0
        },
        {
          "title": "关闭数据自增",
          "block": "关闭后，自增字段不自动增长，用于数据持续录入,保留字段:ignoreIncrese。",
          "type": "switch",
          "key": "ignoreIncrese",
          "unReset": true
        }
      ]
    },
    {
      "title": "记录详情",
      "detail": [
        {
          "title": "轴号1",
          "type": "input",
          "key": "reel_no1",
          "rule": {
            "type": "reel",
            "required": true
          },
          "span": 4,
          "toupper": "true"
        },
        {
          "title": "令数1",
          "type": "input",
          "key": "ream_num1",
          "rule": {
            "type": "int",
            "calc": "小计令数=令数1+令数2+令数3+令数4+令数5+令数6+令数7+令数8+令数9",
            "required": true,
            "msg": "小计令数与记录详情校验失败，两者和不相等"
          },
          "span": 4,
          "defaultValue": 0
        },
        {
          "title": "轴号2",
          "type": "input",
          "key": "reel_no2",
          "rule": {
            "type": "reel"
          },
          "span": 4,
