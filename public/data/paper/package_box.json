{
  "name": "钞纸成品装箱记录",
  "api": {
    "insert": {
      "url": "875/8ab54jm40a",
      "param": ["rec_time", "uid"]
    },
    "update": {
      "url": "876/8ab54jm40a",
      "param": ["cart_num", "patch_no", "box_no", "uid"]
    },
    "query": {
      "url": "877/8ab54jm40a",
      "param": ["cart_num", "patch_no", "box_no"]
    },
    "table": {
      "url": "878/8ab54jm40a",
      "param": ["class_type", "uid"]
    },
    "load": {
      "url": "879/8ab54jm40a",
      "param": ["_id"]
    }
  },
  "table": "paper_package_box",
  "detail": [
    {
      "title": "基础数据",
      "detail": [
        {
          "title": "产品名称",
          "type": "select",
          "unReset": true,
          "defaultOption": [
            "103-G-2T",
            "103-G-2A",
            "103-G-3TZ",
            "103-G-4T",
            "103-G-6T",
            "103-G-7T",
            "试验品"
          ],
          "key": "prod_name",
          "rule": {
            "required": true
          },
          "span": 6
        },
        {
          "title": "码包时间",
          "type": "datepicker",
          "key": "package_date",
          "datetype": "YYYY-MM-DD",
          "rule": {
            "required": true
          },
          "unReset": true,
          "span": 6
        },
        {
          "title": "班次",
          "type": "select",
          "key": "class_type",
          "defaultOption": [
            "光切1班",
            "光切2班",
            "光切3班",
            "光切5班",
            "光切6班",
            "光切7班",
            "光切8班",
            "数封1班",
            "数封2班",
            "打件班"
          ],
          "rule": {
            "required": true,
            "msg": "请输入班次"
          },
          "placeholder": "请输入班次，用拼音缩写快速录入",
          "unReset": true,
          "span": 6
        },
        {
          "title": "班别",
          "type": "radio.button",
          "key": "class_name",
          "rule": {
            "required": true,
            "msg": "请输入班别"
          },
          "defaultOption": ["白班", "中班"],
          "placeholder": "请输入班别",
          "unReset": true,
          "span": 6
        },
        {
          "title": "查纸1",
          "type": "select",
          "mode": "multiple",
          "key": "user_check_paper1",
          "placeholder": "请输入查纸人员",
          "url": "./data/base/paper_cut_user.json",
          "unReset": true,
          "span": 6
        },
        {
          "title": "查纸2",
          "type": "select",
          "key": "user_check_paper2",
          "mode": "multiple",
          "placeholder": "请输入查纸人员",
          "url": "./data/base/paper_cut_user.json",
          "unReset": true,
          "span": 6
        },
        {
          "title": "罩口袋",
          "type": "select",
          "key": "user_package",
          "mode": "multiple",
          "placeholder": "请输入罩口袋人员",
          "url": "./data/base/paper_cut_user.json",
          "unReset": true,
          "span": 6
        },
        {
          "title": "记轴号",
          "type": "select",
          "key": "user_record_reel1",
          "mode": "multiple",
          "placeholder": "请输入记轴号人员",
          "url": "./data/base/paper_cut_user.json",
          "unReset": true,
          "span": 6
        },
        {
          "title": "记轴号2",
          "type": "select",
          "key": "user_record_reel2",
          "mode": "multiple",
          "placeholder": "请输入记轴号人员",
          "url": "./data/base/paper_cut_user.json",
          "unReset": true,
          "span": 6
        },
        {
          "title": "码垛工艺",
          "type": "radio.button",
          "key": "proc_type",
          "defaultOption": [
            {
              "value": "入立体库",
              "name": "入立体库",
              "scope": [
                {
                  "key": "ream_count",
                  "max": 20,
                  "min": 0,
                  "defaultValue": 10
                }
              ],
              "hide": ["patch_no", "box_no"]
            },
            {
              "value": "外调",
              "name": "外调",
              "scope": [
                {
                  "key": "ream_count",
                  "max": 36,
                  "min": 0,
                  "defaultValue": 36
                }
              ],
              "hide": ["cart_num"]
            }
          ],
          "rule": {
            "required": true
          },
          "unReset": true,
          "span": 6
        },
        {
          "title": "批号",
          "type": "input",
          "key": "patch_no",
          "rule": {
            "type": "patch_no",
            "required": true
          },
          "span": 6,
          "unReset": true,
          "toupper": "true"
        },
        {
          "title": "箱号",
          "type": "input.number",
          "key": "box_no",
          "rule": {
            "type": "box_no",
            "required": true
          },
          "span": 6,
          "increase": "box_no"
        },
        {
          "title": "车号",
          "type": "input",
          "key": "cart_num",
          "rule": {
            "type": "reel_cart",
            "required": true
          },
          "span": 6,
          "increase": "reel_cart",
          "toupper": "true"
        },
        {
          "title": "小计令数",
          "type": "input",
          "key": "ream_count",
          "block": "小计令数=令数1+令数2+...+令数8+令数9",
          "rule": {
            "type": "int",
            "required": true
          },
          "span": 6,
          "defaultValue": 0,
          "unReset": true
        },
        {
          "title": "关闭数据自增",
          "block": "关闭后，自增字段不自动增长，用于数据持续录入,保留字段:ignoreIncrese。",
          "type": "switch",
          "key": "ignoreIncrese",
          "unReset": true
        }
      ]
    },
    {
      "title": "记录详情",
      "detail": [
        {
          "title": "轴号1",
          "type": "input",
          "key": "reel_no1",
